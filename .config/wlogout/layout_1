{
    "label" : "lock",
    "action" : "hyprlock",
    "text" : "锁屏",
    "keybind" : "l"
}

{
    "label" : "logout",
    "action" : "hyprctl dispatch exit 0",
    "text" : "注销",
    "keybind" : "e"
}

{
    "label" : "suspend",
    "action" : "systemd-run --user hyprlock && systemctl suspend",
    "text" : "挂起",
    "keybind" : "u"
}

{
    "label" : "shutdown",
    "action" : "systemctl poweroff",
    "text" : "关机",
    "keybind" : "s"
}

